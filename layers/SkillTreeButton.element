element
{
	type = frame-button;
	fileName = SEF/media/frame-bg.png;

	posX = 0;
	posY = 0;
	originX = 0.5;
	originY = 0.5;
	scale = 0.8;
	frameSizeX = 300;
	frameSizeY = 100;
